{
    "name": "simsoft/validator",
    "description": "Validator wrapper for Symfony validator",
    "keywords": ["validator", "symfony", "php", "simsoft"],
    "type": "library",
    "require": {
        "php": "^8",
        "symfony/validator": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Simsoft\\": "src/Validator",
            "Test\\": "tests/Validators"
        }
    },
    "scripts": {
        "test": "phpunit --display-deprecations tests"
    }
}
